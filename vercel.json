{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/about",
      "dest": "/AboutUS.html"
    },
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/pricing",
      "dest": "/pricing.html"
    },
    {
      "src": "/register",
      "dest": "/register.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}