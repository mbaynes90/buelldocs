{
  "name": "buelldocs",
  "version": "1.0.0",
  "description": "BuellDocs - Secure Solutions for Your Future",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --watch src --watch public",
    "preview": "npm run dev & open http://localhost:3000",
    "db:create": "createdb buelldocs",
    "db:init": "psql -d buelldocs -f database/schema.sql",
    "db:reset": "dropdb buelldocs && npm run db:create && npm run db:init"
  },
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}